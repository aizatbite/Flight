<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Test</servlet-name>
        <servlet-class>db.Test</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>passenger</servlet-name>
        <servlet-class>db.passenger</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>checklogin</servlet-name>
        <servlet-class>users.checklogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>insertuser</servlet-name>
        <servlet-class>users.insertuser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateuser</servlet-name>
        <servlet-class>users.updateuser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>checkAdminLogin</servlet-name>
        <servlet-class>admin.checkAdminLogin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Test</servlet-name>
        <url-pattern>/Test</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>passenger</servlet-name>
        <url-pattern>/passenger</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>checklogin</servlet-name>
        <url-pattern>/checklogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>insertuser</servlet-name>
        <url-pattern>/insertuser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateuser</servlet-name>
        <url-pattern>/updateuser</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>insertbooking</servlet-name>
        <servlet-class>Booking.insertbooking</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>insertbooking</servlet-name>
        <url-pattern>/insertbooking</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>checkAdminLogin</servlet-name>
        <url-pattern>/checkAdminLogin</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>insertpayment</servlet-name>
        <servlet-class>Payment.insertpayment</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>insertpayment</servlet-name>
        <url-pattern>/insertpayment</url-pattern>
    </servlet-mapping>

    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
